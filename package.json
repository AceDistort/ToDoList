{
  "name": "to-do-list",
  "version": "0.1.0",
  "scripts": {
    "install": "cd to-do-list-back && npm install && cd ../to-do-list-front && npm install",
    "start:front": "cd to-do-list-front && npm start",
    "start:back": "cd to-do-list-back && npm start",
    "start": "concurrently \"npm run start:back\" \"npm run start:front\""
  },
  "devDependencies": {
    "concurrently": "^8.2.1"
  }
}
